        <Field Name="OrderTotal">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthName">
          <Value>=Switch(Fields!Month.Value = 1, "JAN", Fields!Month.Value = 2, "FEB", Fields!Month.Value = 3, "MAR", Fields!Month.Value = 4, "APR", Fields!Month.Value = 5, "MAY", Fields!Month.Value = 6, "JUN", Fields!Month.Value = 7, "JUL", Fields!Month.Value = 8, "AUG", Fields!Month.Value = 9, "SEP", Fields!Month.Value = 10, "OCT", Fields!Month.Value = 11, "NOV", Fields!Month.Value = 12, "DEC")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NoSqlAdventureWorks</DataSourceName>
        <CommandText>FROM VIEW(aw,sales_by_region_year_month)
USING QUERYOPTIONS(Group=true)
WITH KEY[GroupName,CountryCode,Country,Year,Month]
WHERE	Year = @Year
AND	Month BETWEEN(@StartMonth,@EndMonth)
AND	GroupName BETWEEN(MIN_STRING,MAX_STRING)
AND	CountryCode BETWEEN(MIN_STRING,MAX_STRING)
AND	Country BETWEEN(MIN_STRING,MAX_STRING)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartMonth">
            <Value>=Parameters!StartMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndMonth">
            <Value>=Parameters!EndMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
